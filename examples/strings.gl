( define concat (lambda (a b)
		 (
		 toString 
		 (
		 ++
		 (toCharList a)
		 (toCharList b)
		 )
		 )

		)
)

(define concatList (lambda (my_list)
    (toString (++ (map toString my_list)))
    )
)


(print (concat "hey" "hyo"))
(print "----------------------")
(print (concatList (list "Hey" " " "This Is" "\n" "A" "\n" "TEST")))
(print "----------------------")
